{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["eslint:recommended","prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "max-len": ["error", { "code": 180 }],
        "one-var-declaration-per-line": ["error", "always"]
    }
}
